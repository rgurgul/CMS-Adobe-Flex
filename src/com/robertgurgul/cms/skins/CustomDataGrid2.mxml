<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx" 
		xmlns:przyklad1="pl.gov.men.sio2.szkolenie.komponenty.przyklad1.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("pl.gov.men.sio2.szkolenie.komponenty.przyklad2.CustomDataGrid2")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="normal" />
		<s:State name="disable" />
	</s:states>
	
	<s:VGroup paddingTop="10">
		<s:HGroup visible.normal="true" visible.disable="false">
			<przyklad1:CustomComboBox id="combo"/>
		</s:HGroup>
		<s:DataGrid id="grid" width="100%" height="100%"> 
			<s:columns>
				<s:ArrayCollection >
					<s:GridColumn headerText="Name1" />
					<s:GridColumn headerText="Name2" />
					<s:GridColumn headerText="Name3" visible="false" />
					<s:GridColumn headerText="Name4" />
					<s:GridColumn headerText="Name5" />
					<s:GridColumn headerText="Name6" />
					<s:GridColumn headerText="Name7" />
				</s:ArrayCollection>
			</s:columns>
		</s:DataGrid>
	</s:VGroup>
</s:Skin>
