<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx" 
		xmlns:przyklad1="pl.gov.men.sio2.szkolenie.komponenty.przyklad1.*">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("com.robertgurgul.cms.components.CustomDataGridSkin")]
	</fx:Metadata>
	
	<fx:Declarations>
		<s:ArrayList id="dp"> 
			<fx:Object> 
				<fx:Artist>Pavement</fx:Artist> 
				<fx:Price>11.99</fx:Price> 
				<fx:Album>Slanted and Enchanted</fx:Album> 
			</fx:Object> 
			<fx:Object> 
				<fx:Price>11.99</fx:Price> 
				<fx:Artist>Pavement</fx:Artist> 
				<fx:Album>Brighten the Corners</fx:Album> 
			</fx:Object> 
		</s:ArrayList>
	</fx:Declarations>
	
	<!-- states -->
	<s:states>
		<s:State name="normal" />
		<s:State name="disable" />
	</s:states>
	
	<s:VGroup paddingTop="10">
		
		<s:DataGrid id="grid" left="10" right="10" dataProvider="{dp}">
			
		</s:DataGrid>
		
	</s:VGroup>
</s:Skin>
